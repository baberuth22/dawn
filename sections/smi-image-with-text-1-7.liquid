{%- liquid
  assign media_height = section.settings.media_height
  assign collapse_index = 0
  assign media_shape = section.settings.media_shape
  assign content_position = section.settings.content_position
  assign color_pos = 0
  assign bkg_pos = 0
  for block in section.blocks
  if block.type == 'color'
  assign color_pos = forloop.index
  endif
  if block.type == 'background'
  assign bkg_pos = forloop.index
  endif
endfor
-%}
{%-liquid
    assign media_ratio = section.settings.media_ratio
    assign media_shape = section.settings.media_shape
    if media_shape == 'none'
        assign media_shape = 'hidden'
    endif
    assign media_classes = 'smi-w-100 smi-object-fit-cover '
    if media_shape != 'default' and media_shape != 'round'
        assign media_classes = media_classes | append: media_shape
    endif
    if media_ratio == 'auto' 
        assign media_classes = media_classes | append: ' smi-h-auto'
    else
        assign media_classes = media_classes | append: ' smi-h-100 smi-p-absolute'
    endif
    assign media_wrapper_classes = 'smi-media-wrapper smi-border smi-shadow smi-lh-0 smi-overflow-hidden smi-p-relative smi-' | append: media_shape
    if media_shape contains 'blob'
        assign media_wrapper_classes = media_wrapper_classes | append: ' smi-blob-wrapper smi-has-blob'
    endif
-%}
<div
  id="smi-{{ section.id }}"
  class="smi-{{ section.id }} smi-section__inner smi-gradient smi-section-image-with-text-1 smi-color-{{ section.settings.color_scheme }} smi-content-settings"
  data-section="image-with-text-1" data-name="✧Image with text 1">
   
  {% render 'smi-background' %}
  <input type="hidden" value="" id="interval_countdown_{{ section.id }}">
  <div class="smi-container-relative smi-{{ section.settings.full_width }}">
    <div class="smi-container-detail smi-container-detail-square-{{ section.settings.desktop_media_width }} smi-content-position-{{ section.settings.content_position }}">
      <div class="
        smi-section-block-{{ section.id }}
        smi-content-detail-padding-{{ section.settings.desktop_media_position }}
        smi-rich-content-detail smi-w-100
        smi-rich-content-{{ 100 | minus: section.settings.desktop_media_width }}
        smi-w-md-{{ 100 | minus: section.settings.desktop_media_width }}
        smi-w-md-{{ 100 | minus: section.settings.desktop_media_width }}-content
        smi-rich-content-load1 content-detail-{{ section.settings.full_width }}"
        data-load='
          {
            "size": "{{ section.blocks.size }}" 
          }
        '
