            {% if block.type != 'color' and block.type != 'background' %}
              {% if block.type == 'button' %} smi-mb-md
              {%else %}smi-mb-lg
            {% endif %} 
            {% endif %} 
              smi-block-type-{{ block.type }}
              "
              {{ block.shopify_attributes }} 
          >
            {% if block.type == 'heading' %}
              <div class="smi-component-blocks-heading content-alignment-{{ content_alignment }}">
