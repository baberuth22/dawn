{%- liquid
  assign grid_horizontal = settings.smi_spacing_grid_horizontal
  assign blocks = section.blocks | where: 'type', 'testimonial_block'
  assign desktop_items = section.settings.number_of_item | times: 1
  assign change_slide_every = section.settings.change_slide_every | times: 1000
  assign arr_slider = true
  assign page_dot_style = section.settings.sticky_page_dot_style
  if blocks.size < desktop_items
    assign desktop_items = blocks.size
  endif
  assign tablet_items = 2
  if desktop_items == 1
    assign tablet_items = 1
  endif
  if blocks.size <= desktop_items
    assign arr_slider = false
  endif
-%}
